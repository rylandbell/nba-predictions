{
  "name": "nba_survivor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "heroku-postbuild": "webpack -p --config --progress",
    "updateData": "node ./bin/updateByDate.js",
    "build-watch": "watchify -t [babelify] -x react -x react-dom -x redux -x expect -e src/**/main*.js* -e src/misc/*.js -o public/javascripts/bundle.js",
    "build-vendor": "browserify -t [babelify] -r react -r react-dom -r redux -r expect -o /public/javascripts/vendor.js",
    "sass-watch": "sass --watch scss:css",
    "<ser></ser>ve": "webpack-dev-server"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "basic-auth": "^1.0.3",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "crypto": "0.0.3",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "express": "~4.13.1",
    "express-jwt": "^5.0.0",
    "express-logger": "0.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "minimatch": "^3.0.2",
    "moment": "^2.15.0",
    "moment-timezone": "^0.5.7",
    "mongoose": "^4.6.0",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-s-alert": "^1.2.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.71.0",
    "serve-favicon": "~2.3.0",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "engines": {
    "node": "~4.2.6",
    "npm": "~2.14.12"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "expect": "^1.20.2",
    "file-loader": "^0.9.0",
    "grunt": "~0.4.5",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-jscs": "^3.0.0",
    "grunt-puglint": "^0.1.3",
    "mocha": "^2.5.3"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "mocha": true,
      "jquery": true
    },
    "globals": {
      "moment": false,
      "activeMonth": false
    },
    "plugins": [
      "react"
    ],
    "react": {
      "pragma": "React",
      "version": "15.0"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/display-name": 0,
      "no-console": 0
    }
  }
}
